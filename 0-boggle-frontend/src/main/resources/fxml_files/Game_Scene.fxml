<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="500.0" minWidth="750.0" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Pane fx:id="textInputPane" layoutX="48.0" layoutY="46.0" prefHeight="454.0" prefWidth="459.0">
      <Label fx:id="textInputLabel" alignment="CENTER" layoutX="68.0" layoutY="320.0" prefHeight="48.0" prefWidth="324.0" text="Tippe dein Wort hier ein...">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="eventButton" layoutY="380.0" mnemonicParsing="false" onAction="#eventButtonClicked" prefHeight="31.0" prefWidth="459.0" text="Starten">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <ProgressBar fx:id="timerProgress" disable="true" layoutY="380.0" prefHeight="31.0" prefWidth="459.0" progress="1.0" visible="false" />
      <Label fx:id="timerLabel" alignment="CENTER" disable="true" layoutX="195.0" layoutY="411.0" prefHeight="31.0" prefWidth="70.0" text="03:00" textFill="RED" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Pane fx:id="gamePane" layoutX="70.0" prefHeight="320.0" prefWidth="320.0" />
   </Pane>
   <ListView fx:id="guessListView" fixedCellSize="30.0" focusTraversable="false" layoutX="543.0" prefHeight="500.0" prefWidth="207.0" />
</AnchorPane>
